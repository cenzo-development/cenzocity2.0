<!-- Begin page -->
       <div id="wrapper">

           <!-- ========== Left Sidebar Start ========== -->
           <div class="left side-menu">

               <div class="slimscroll-menu" id="remove-scroll">

                   <!-- LOGO -->
                   <div class="topbar-left">
                     <%= link_to root_path, class: "logo" do %>
                           <span>
                               <%= image_tag "cenzo.png", alt: "", height: "80" %>
                           </span>
                           <% end %>
                   </div>

                   <!-- User box -->
                   <div class="user-box">

                     <h5><%=link_to current_user.first_name + " " + current_user.last_name, "#"%></h5>
                       <p class="text-muted">Admin Head</p>
                   </div>

                   <!--- Sidemenu -->
                   <div id="sidebar-menu">

                       <ul class="metismenu" id="side-menu">

                           <!--<li class="menu-title">Navigation</li>-->
                           <li>
                               <%= link_to root_path do %>
                                   <i></i><span></span> <span> Dashboard </span>
                                   <% end %>
                           </li>

                           <li>
                               <%= link_to root_path do %>
                                   <i></i><span ></span> <span> Kof </span>
                                   <% end %>
                           </li>

                           <li>
                             <%= link_to root_path do %>
                                 <i></i><span ></span> <span> Clients </span>
                                 <% end %>
                           </li>

                           <li>
                             <%= link_to root_path do %>
                                 <i></i><span></span> <span> Organisations </span>
                                 <% end %>
                           </li>

                           <li>
                             <%= link_to root_path do %>
                                 <i></i><span></span> <span> Treatments </span>
                                 <% end %>
                           </li>


                           <li>
                             <%= link_to root_path do %>
                                 <i></i><span></span> <span> Invoices </span>
                                 <% end %>
                           </li>

                           <li>
                             <%= link_to root_path do %>
                                 <i></i><span></span> <span> Management </span>
                                 <% end %>
                           </li>

                       </ul>

                   </div>
                   <!-- Sidebar -->
               </div>
               <!-- Sidebar -left -->

           </div>
           <!-- Left Sidebar End -->
           <!-- ============================================================== -->
           <!-- Start right Content here -->
           <!-- ============================================================== -->

           <div class="content-page">

                 <!-- Top Bar Start -->
                 <div class="topbar">

                     <nav class="navbar-custom">

                         <ul class="list-unstyled topbar-right-menu float-right mb-0">

                             <li class="hide-phone app-search">

                               <form>
                                   <input type="text" placeholder="Search..." class="form-control">
                                   <button type="submit"><i class="fa fa-search"></i></button>
                               </form>

                             </li>

                             <li class="dropdown notification-list">

                               <a class="nav-link dropdown-toggle arrow-none" data-toggle="dropdown" href="#" role="button"
                                  aria-haspopup="false" aria-expanded="false"> <span class="ml-1"><%=current_user.first_name%><i class="fa fa-chevron-down"></i> </span>
                               </a>

                                 <div class="dropdown-menu dropdown-menu-right ">
                                     <!-- item-->
                                     <div class="dropdown-item noti-title">
                                         <h6 class="text-overflow m-0">Welcome !</h6>
                                     </div>

                                     <!-- item-->
                                     <%= link_to "Change Password", root_path, class: "dropdown-item notify-item" %>

                                     <!-- item-->
                                     <%= link_to "Logout", logout_path, class: "dropdown-item notify-item", method: :delete %>
                                 </div>
                             </li>

                         </ul>

                         <ul class="list-inline menu-left mb-0">
                             <li class="float-left">
                                 <button class="button-menu-mobile open-left disable-btn">
                                     <i class="dripicons-menu"></i>
                                 </button>
                             </li>
                             <li>
                                 <div class="page-title-box">
                                     <h4 class="page-title">Dashboard </h4>
                                 </div>
                             </li>

                         </ul>

                     </nav>

                 </div>
                 <!-- Top Bar End -->

               <!-- Start Page content -->

           <!-- ============================================================== -->
           <!-- End Right content here -->
           <!-- ============================================================== -->

       </div>
       <!-- END wrapper -->
